{
    "name": "FFTRadNet_matlab",
    "seed":3,
    "model": {
        "backbone_block": [3, 6, 6, 3],
        "channels": [32,40,48,56],
        "MIMO_output": 192,
        "DetectionHead": "True",
        "SegmentationHead": "False"
    },
    "dataset": {
        "root_dir": "/media/julien/HD_RADAR1/FinalDataset/radar_v1/RADIal/",
        "geometry":{
            "ranges": [512,896,1],
            "resolution": [0.201171875,0.2],
            "size": 15
        },
        "statistics":{
            "input_mean":[-8.12419793e-18, -2.99906584e-17, -3.15557042e-17,  2.21559424e-17],
            "input_std":[9.56788384e-17, 3.13128113e-16, 2.11709611e-16, 1.21820849e-16],
            "reg_mean":[0.232915, 0.226690],
            "reg_std":[0.240285, 0.232990]
        }
    },
    "losses": {
        "fct":"PixorLoss",
        "classification": "FocalLoss",
        "regression": "smoothL1Loss",
        "weight": [1,100,100]
    },
    "lr_scheduler": {
        "step_size": 10,
        "gamma": 0.9
    },
    "optimizer": {
        "lr": 1e-4
    },
    "num_epochs":100,
    "dataloader": {
        "mode":"random",
        "split":[0.7,0.15,0.15],
        "train": {
            "batch_size": 4,
            "num_workers": 4
    	},
        "val": {
            "batch_size": 4,
            "num_workers": 4
        },
        "test": {
            "batch_size": 1,
            "num_workers": 1
        }
    },
    "output": {
	"dir": "../"
    }
}
